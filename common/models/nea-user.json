{
  "name": "neaUser",
  "base": "User",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "username": {
      "type": "string",
      "required": true,
      "id": true,
      "generated": false
    },
    "email": {
      "type": "string",
      "required": true
    },
    "name": {
      "type": "string"
    },
    "surname": {
      "type": "string"
    },
    "avatar": {
      "type": "string"
    },
    "biography": {
      "type": "string"
    },
    "sex": {
      "type": "string"
    },
    "bornDate": {
      "type": "date"
    },
    "city": {
      "type": "string"
    },
    "favFood": {
      "type": "string"
    },
    "interests": {
      "type": "string"
    },
    "job": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "invitesSent": {
      "type": "hasMany",
      "model": "Invite",
      "foreignKey": "sourceUser"
    },
    "invitesReceived": {
      "type": "hasMany",
      "model": "Invite",
      "foreignKey": "targetUser"
    }
  },
  "acls": [
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
